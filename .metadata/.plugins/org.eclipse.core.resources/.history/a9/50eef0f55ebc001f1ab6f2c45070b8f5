package PAIRS1E;

import java.util.Scanner;

public class Main {

	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		int t = Integer.parseInt(sc.nextLine());
		for (int testcase = 1; testcase <= t; testcase++) {
			int N = sc.nextInt();
			int K = sc.nextInt();
			
			int[] arr = new int[N];
			for(int i = 0 ; i < N ; i++) {
				arr[i] = sc.nextInt();
			}
		}

	}

}
